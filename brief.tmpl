  <a href="{url}{sessionvars%urlparameter}rev/{node}" class="revision">{rev}:{node}</a>
  <span class="tag branchhead">{branch}</span>
  <span class="tag">{tags}</span>
  {bookmarks % '<span class="tag">{name|escape}</span> '}
  <span>{description|redmine}</span>
  <span class="description">{description|firstline|fill68}</span>
